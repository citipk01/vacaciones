SELECT   A.COD_MF,
		A.PERIODO,
		 A.COD_EMP,
		 B.NOM_EMP,
		 C.NOM_MF,
         NVL(A.DIAS_CALCULADOS,0) DIAS_CALCULADOS,
         nvl(A.DIAS_TOMADOS,0) DIAS_TOMADOS,
         NVL(A.MAX_FRAG,0) MAX_FRAG,
         nvl(nvl(A.DIAS_CALCULADOS,0) - nvl(A.DIAS_TOMADOS,0),0) DIAS_PENDIENTES
FROM   BASEARG.CB_VAC A,
       BASEARG.QSEMPRESA B,
       BASEARG.MAEFUNC2 C
WHERE   A.COD_EMP = B.COD_EMP
       AND A.COD_MF = C.COD_MF
       AND C.FEC_EGR_MF IS NULL
       AND A.PERIODO = @PERIODO.
       AND A.COD_EMP = @COD_EMP.
       AND A.COD_MF = (SELECT COD_MF FROM CB_SID WHERE S_ID = @BXTOKEN. )
	</ORA>
				<INF>
				</INF>
				<OUTPUT/>
				<ACCESSRULES><ACCESSRULE ID="MIFORMVAC"/></ACCESSRULES>
				<LANG/>
			</DATA><DATA delimL="@" delimR="." EDATE="01/01/1900" ID="CITI.MIFORMVAC.SEL" NAME="Busqueda" GROUP="CITI.MIFORMVAC" VERSION="1.1" UED="19000101" ORD="">
				<DESCRIPTION></DESCRIPTION>
				<PARAMETERS>
					<PARAMETER parOrder="1" id="BXTOKEN" parType="1" parTypeName="String"/>
				</PARAMETERS>
				<SQL>
				</SQL>
