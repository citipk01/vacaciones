SELECT   A.COD_MF,
		A.PERIODO,
		 A.COD_EMP,
		 B.NOM_EMP,
		 C.NOM_MF,
         A.DIAS_CALCULADOS,
         A.DIAS_TOMADOS,
         A.MAX_FRAG,
         A.DIAS_CALCULADOS - A.DIAS_TOMADOS DIAS_PENDIENTES
FROM   BASEARG.CB_VAC A,
       BASEARG.QSEMPRESA B,
       BASEARG.MAEFUNC2 C
WHERE   A.COD_EMP = B.COD_EMP
       AND A.COD_MF = C.COD_MF
       AND 1 &lt;&gt; 1
	</ORA>
				<INF>
