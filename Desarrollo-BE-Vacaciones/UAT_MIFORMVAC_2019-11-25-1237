SELECT   A.PERIODO,
		 A.COD_EMP,
		 B.NOM_EMP EMPRESA,
         A.DIAS_CALCULADOS CALCULADOS,
         A.DIAS_TOMADOS TOMADOS,
         A.MAX_FRAG,
         A.DIAS_CALCULADOS - A.DIAS_TOMADOS PENDIENTES
FROM   BASEARG.CB_VAC A,
       BASEARG.QSEMPRESA B,
       BASEARG.MAEFUNC2 C
WHERE   A.COD_EMP = B.COD_EMP
       AND A.COD_MF = C.COD_MF
       AND A.PERIODO BETWEEN TRUNC (SYSDATE, 'YY') - 1 AND  ADD_MONTHS (TRUNC (SYSDATE, 'YEAR'), 12) - 1 
       AND A.COD_MF = (SELECT COD_MF FROM CB_SID WHERE S_ID = @BXTOKEN. )
	</ORA>
