				<ORA>
SELECT   A.PERIODO,
		 A.COD_EMP,
		 B.NOM_EMP EMPRESA,
         A.DIAS_CALCULADOS CALCULADOS,
         A.DIAS_TOMADOS TOMADOS,
         A.MAX_FRAG,
         A.DIAS_CALCULADOS - A.DIAS_TOMADOS PENDIENTES
FROM   BASEARG.CB_VAC A,
       BASEARG.QSEMPRESA B,
       BASEARG.MAEFUNC2 C
WHERE   A.COD_EMP = B.COD_EMP
       AND A.COD_MF = C.COD_MF
       AND C.FEC_EGR_MF IS NULL
       AND A.PERIODO BETWEEN TRUNC (SYSDATE, 'YY') - 1 AND  ADD_MONTHS (TRUNC (SYSDATE, 'YEAR'), 12) - 1 
       AND A.COD_MF = (SELECT COD_MF FROM CB_SID WHERE S_ID = @BXTOKEN. )
	</ORA>
				<INF>
				</INF>
				<OUTPUT/>
				<ACCESSRULES><ACCESSRULE ID="MIFORMVAC"/></ACCESSRULES>
				<LANG/>
			</DATA><DATA delimL="@" delimR="." EDATE="01/01/1900" ID="CITI.MIFORMVAC.SEL.INI" NAME="Inicializa busqueda" GROUP="CITI.MIFORMVAC" VERSION="1.1" UED="19000101" ORD="">
				<DESCRIPTION></DESCRIPTION>
				<PARAMETERS>
					<PARAMETER parOrder="1" id="BXTOKEN" parType="1" parTypeName="String"/>
				</PARAMETERS>
				<SQL>
				</SQL>
